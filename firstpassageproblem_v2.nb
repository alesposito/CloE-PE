Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"pxyz", "[", "k_", "]"}], "=", 
   RowBox[{"Normal", "[", 
    RowBox[{"Series", "[", 
     RowBox[{
      RowBox[{"PDF", "[", 
       RowBox[{
        RowBox[{"\[ScriptCapitalP]", "[", "k", "]"}], ",", "8"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"p", ",", "0", ",", "3"}], "}"}]}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Pxyz", "[", "k_", "]"}], "=", 
   RowBox[{"1", "-", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "N"}], " ", 
      RowBox[{"pxyz", "[", "k", "]"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"PDFxyz", "[", "k_", "]"}], "=", 
   RowBox[{"D", "[", 
    RowBox[{
     RowBox[{"Pxyz", "[", "k", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"k", ",", "1"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"km", "=", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"k", " ", 
      RowBox[{"PDFxyz", "[", "k", "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"k", ",", "0", ",", "\[Infinity]"}], "}"}], ",", 
     RowBox[{"Assumptions", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"N", ">", "1"}], ",", 
        RowBox[{"p", ">", "0"}], ",", 
        RowBox[{"p", "<", "1"}], ",", 
        RowBox[{"\[CapitalOmega]", " ", ">", "0"}], ",", " ", 
        RowBox[{"x", ">", "0"}], ",", " ", 
        RowBox[{"y", ">", "0"}], ",", 
        RowBox[{"z", ">", "0"}]}], "}"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ta", "=", 
   RowBox[{"km", "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"x", "\[Rule]", "1"}], ",", 
      RowBox[{"y", "\[Rule]", "1"}], ",", 
      RowBox[{"z", "\[Rule]", "1"}]}], "}"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"t\[CapitalOmega]", "=", 
   RowBox[{"km", "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"x", "\[Rule]", "1"}], ",", 
      RowBox[{"y", "\[Rule]", "1"}], ",", 
      RowBox[{"z", "\[Rule]", "\[CapitalOmega]"}]}], "}"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Simplify", "[", 
  RowBox[{
